    flex-basis: calc(16% - 16px); /* Учитываем отступ между элементами */
    text-align: center;
    margin-bottom: 20px;
    margin-right: 16px; /* Добавляем отступ между элементами */
    max-width: calc(16% - 16px); /* Учитываем отступ между элементами */
    box-sizing: border-box;
}

