    $('.persona3').on('click', function() {
      window.location.href = 'mailto:diana.p@regulatedunitedeurope.com';
    });

    $('.persona4').on('click', function() {
      window.location.href = 'mailto:polina.m@regulatedunitedeurope.com';
    });
  });
</script>

