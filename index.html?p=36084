<li id="menu-item-21596" class="menu-item menu-item-type-custom menu-item-object-custom menu-item-21596"><a href="https://regulatedunitedeurope.com/de/kontakt-us/">Kontaktiere uns</a></li>
                
                              <!-- SEARCH ICON -->
              <li>
				  <a  href="#" class="mt-search-icon">
                <i class="fa fa-search" aria-hidden="true"></i>
              </a>	
</li>
